@{
    Layout = "";
    ViewBag.Title = "Home Page";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/HomeCoverCss")
    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")




</head>

<body>
    <div class="site-wrapper">
        <div class="site-wrapper-inner">
            <div class="cover-container">
                <div class="inner cover">
                    <h1 class="cover-heading">EasyAD，你的专属推广资源库</h1>
                    <form class="form-inline" role="form">
                        <div class="form-group"><input type="text" name="keyword" id="keyword" class="form-control input-lg" placeholder="请输入关键词" /></div>
                        <input type="submit" value="搜索" class="btn btn-lg btn-primary" />
                    </form>
                </div>
            </div>
        </div>
    </div>

    <footer class="panel-footer navbar-fixed-bottom">123</footer>

</body>
</html>
